<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <ripple android:color="@color/colorSecondary03LightBlue">
            <item>
                <shape android:shape="rectangle">

                    <gradient
                        android:type="linear"
                        android:startColor="#FEBE80"
                        android:endColor="#FE8D90"
                        android:angle="270" />

                    <corners
                        android:topLeftRadius="25dp"
                        android:bottomLeftRadius="25dp"
                        />

                </shape>
            </item>
        </ripple>

    </item>
    <item
        android:drawable="@drawable/ic_cat_food_on"
        android:top="14dp"
        android:right="24dp"
        android:left="24dp"
        android:bottom="14dp">
    </item>
</layer-list>


